@page "/"
@using CarMarket.UI.Data

<h1>Hello, world!</h1>

<div class="alert alert-warning" role="alert">
    Before authentication will function correctly, you must configure your provider details in <code>Program.cs</code>
</div>

Welcome to your new app.

<SurveyPrompt Title="How is Blazor working for you?" />
<BreadCrumb BreadCrumbData="BreadCrumbDatas"></BreadCrumb>

@code{
    public List<BreadCrumbData> BreadCrumbDatas = new List<BreadCrumbData>();

    protected override void OnInitialized()
    {
        BreadCrumbDatas.Add(new BreadCrumbData()
        {
            Text = "Home",
            Url = "/"
        });
    }
}